@require 'parts/prettify'
li.L0, li.L1, li.L2, li.L3, li.L5, li.L6, li.L7, li.L8
	list-style-type decimal !important


body
	background #f2f2f2

#main
	min-height 1600px
	background #fff

#menus .lists a
	padding 10px 25px
	display block
	border-radius 5px
	&:hover
		background #ddd
		
	&.active
		background #08c
		color #eee

.loading
	position fixed
	background #333
	color #fff
	font-size 12px
	border-radius 4px 0 0 4px
	padding 4px
	right 0
	top 5px